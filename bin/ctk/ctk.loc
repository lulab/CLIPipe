#<dbkey> <analysis> <path> [type]


#***hg38***

#for bed2annotation.pl
hg38 gene	annotation/genes/gene2symbol.hg.20180317 gene2symbol
hg38 gene	annotation/genomes/hg38/annotation/refGene_knownGene.bed transcript
hg38 gene	annotation/genomes/hg38/annotation/refGene_knownGene_To_gene.txt transcript2gene
hg38 rmsk	annotation/genomes/hg38/annotation/rmsk.bed.gz
hg38 rmsk_RNA	annotation/genomes/hg38/annotation/rmsk.RNA.bed
hg38 miRNA	annotation/genomes/hg38/annotation/miRNA.bed


#note: these are non-overlapping regions after clustering, to reduce computing time
hg38 region	annotation/genomes/hg38/annotation/hg38.genic.bed genic

hg38 region	annotation/genomes/hg38/annotation/refGene_knownGene.upstream.10k.cluster.bed upstream_10k
hg38 region	annotation/genomes/hg38/annotation/refGene_knownGene.downstream.10k.cluster.bed downstream_10k

hg38 region	annotation/dbCASE/db_hg38/exons/hg38.exon.uniq.cluster.bed exon

hg38 region	annotation/genomes/hg38/annotation/refGene_knownGene.5utr.cluster.bed 5utr
hg38 region	annotation/genomes/hg38/annotation/refGene_knownGene.3utr.cluster.bed 3utr
hg38 region	annotation/genomes/hg38/annotation/refGene_knownGene.cds.cluster.bed cds

#for tag2peak
hg38 genic	annotation/genomes/hg38/annotation/refGene_knownGene.meta.6.bed




